{
  "$schema": "https://turborepo.com/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".nitro/**", ".output/**", ".tanstack/**"]
    },
    "dev": {
      "persistent": true
    },
    "dev:i18n:extract": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": false
    },
    "dev:i18n:types": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": false
    }
  }
}
